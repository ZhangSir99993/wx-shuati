<view class="container">
    <view class="title_list">
        <view class="title_item" wx:for="{{itemList}}"  wx:key="index" data-albumid="{{item.albumId}}">
            <view class="albumId1"  data-albumid="{{item.albumId}}" catchtap="detailClick">{{item.albumId}}</view>
            <view class="albumId2" wx:for="{{item.albumId2}}" wx:key="key" wx:for-item="value"  data-albumid="{{item.albumId}}" data-albumid2="{{value.albumId2}}"  catchtap="detailClick">
                {{value.albumId2}}
                <view class="albumId2" wx:for="{{value.albumId3}}" wx:key="j" wx:for-item="albumId3"  data-albumid="{{item.albumId}}" data-albumid2="{{value.albumId2}}" data-albumid3="{{albumId3}}" catchtap="detailClick">
                    {{albumId3}}
                    <image class="arrowright" src="../../images/arrowright.png" />
                </view>
                 <image class="arrowright" src="../../images/arrowright.png" />
            </view>
             <image class="arrowright" src="../../images/arrowright.png" />
        </view>
    </view>
</view>
